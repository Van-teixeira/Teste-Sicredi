        <add assembly="System.Design, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
      </assemblies>
    </compilation>
    -->
    <pages>
      <controls>
        <!--<add assembly="JUnit.WebCommon" namespace="JUnit.Web.Controls" tagPrefix="JUnit"/>-->
